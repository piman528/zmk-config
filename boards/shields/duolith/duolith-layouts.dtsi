#include <physical_layouts.dtsi>

/ {
    default_layout: default_layout {
        compatible = "zmk,physical-layout";
        display-name = "Default Layout";
        transform = <&default_transform>;
        kscan = <&kscan>;

        keys  //                                             w   h    x     y    rot   rx   ry
            // Row 0: Left (6 cols: 5 keys + gap), Right (5 cols)
            = <&key_physical_attrs                           100 100     0     0      0     0    0>  // SW1  (L c0)
            , <&key_physical_attrs                           100 100   100     0      0     0    0>  // SW2  (L c1)
            , <&key_physical_attrs                           100 100   200     0      0     0    0>  // SW3  (L c2)
            , <&key_physical_attrs                           100 100   300     0      0     0    0>  // SW4  (L c3)
            , <&key_physical_attrs                           100 100   400     0      0     0    0>  // SW5  (L c4)
            , <&key_physical_attrs                           100 100   500     0      0     0    0>  // GAP  (L c5 filler)
            , <&key_physical_attrs                           100 100   900     0      0     0    0>  // SW22 (R c0, base 800 + x:1)
            , <&key_physical_attrs                           100 100  1000     0      0     0    0>  // SW23 (R c1)
            , <&key_physical_attrs                           100 100  1100     0      0     0    0>  // SW24 (R c2)
            , <&key_physical_attrs                           100 100  1200     0      0     0    0>  // SW25 (R c3)
            , <&key_physical_attrs                           100 100  1300     0      0     0    0>  // SW26 (R c4)

            // Row 1
            , <&key_physical_attrs                           100 100     0   100      0     0    0>  // SW6  (L c0)
            , <&key_physical_attrs                           100 100   100   100      0     0    0>  // SW7  (L c1)
            , <&key_physical_attrs                           100 100   200   100      0     0    0>  // SW8  (L c2)
            , <&key_physical_attrs                           100 100   300   100      0     0    0>  // SW9  (L c3)
            , <&key_physical_attrs                           100 100   400   100      0     0    0>  // SW10 (L c4)
            , <&key_physical_attrs                           100 100   500   100      0     0    0>  // GAP  (L c5 filler)
            , <&key_physical_attrs                           100 100   900   100      0     0    0>  // SW27 (R c0, base 800 + x:1)
            , <&key_physical_attrs                           100 100  1000   100      0     0    0>  // SW28 (R c1)
            , <&key_physical_attrs                           100 100  1100   100      0     0    0>  // SW29 (R c2)
            , <&key_physical_attrs                           100 100  1200   100      0     0    0>  // SW30 (R c3)
            , <&key_physical_attrs                           100 100  1300   100      0     0    0>  // SW31 (R c4)

            // Row 2
            , <&key_physical_attrs                           100 100     0   200      0     0    0>  // SW11 (L c0)
            , <&key_physical_attrs                           100 100   100   200      0     0    0>  // SW12 (L c1)
            , <&key_physical_attrs                           100 100   200   200      0     0    0>  // SW13 (L c2)
            , <&key_physical_attrs                           100 100   300   200      0     0    0>  // SW14 (L c3)
            , <&key_physical_attrs                           100 100   400   200      0     0    0>  // SW15 (L c4)
            , <&key_physical_attrs                           100 100   500   200      0     0    0>  // GAP  (L c5 filler)
            , <&key_physical_attrs                           100 100   900   200      0     0    0>  // SW32 (R c0, base 800 + x:1)
            , <&key_physical_attrs                           100 100  1000   200      0     0    0>  // SW33 (R c1)
            , <&key_physical_attrs                           100 100  1100   200      0     0    0>  // SW34 (R c2)
            , <&key_physical_attrs                           100 100  1200   200      0     0    0>  // SW35 (R c3)
            , <&key_physical_attrs                           100 100  1300   200      0     0    0>  // SW36 (R c4)

            // Row 3
            , <&key_physical_attrs                           100 100     0   300      0     0    0>  // SW16 (L c0)
            , <&key_physical_attrs                           100 100   100   300      0     0    0>  // SW17 (L c1)
            , <&key_physical_attrs                           100 100   200   300      0     0    0>  // SW18 (L c2)
            , <&key_physical_attrs                           100 100   300   300      0     0    0>  // SW19 (L c3)
            , <&key_physical_attrs                           100 100   400   300      0     0    0>  // SW20 (L c4)
            , <&key_physical_attrs                           100 100   500   300      0     0    0>  // SW21 (L c5)
            , <&key_physical_attrs                           100 100   800   300      0     0    0>  // SW37 (R c0, base 800)
            , <&key_physical_attrs                           100 100   900   300      0     0    0>  // SW38 (R c1)
            , <&key_physical_attrs                           100 100  1000   300      0     0    0>  // GAP  (R c2 filler, x:2 gap)
            , <&key_physical_attrs                           100 100  1100   300      0     0    0>  // SW39 (R c3)
            , <&key_physical_attrs                           100 100  1200   300      0     0    0>  // SW40 (R c4)
            ;
    };
};
